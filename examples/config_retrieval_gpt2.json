{
    "data": {
        "train_data_path": "./datasets/wiki-2-processed-sampled/train",
        "test_data_path": "./datasets/wiki-2-processed-sampled/test_sampled_50",
        "type": "unsupervised"
    },
    "influence": {
        "outdir": "output_dir",
        "seed": 42,
        "cal_words_infl": false,
        "n_threads": 1,
        "RapidGrad": {
            "enable": true,
            "RapidGrad_K": 65536,
            "shuffle_lambda": 20 
        },
        "offload_test_grad": false,
        "offload_train_grad": false,
        "delete_model": true,
        "calculate_infl_in_gpu": true,
        "load_from_grads_path": true,
        "save_to_grads_path": false,
        "grads_path": "./grads_path_gpt2/",
        "top_k": 1000
    },
    "model": {
        "model_path": "openai-community/gpt2",
        "checkpoint": "./checkpoints/gpt2-wiki-2-checkpoint360/",
        "lora_path": "null",
        "max_length": 512,
        "load_in_4bit": true
    }
}
